<h1> <%= @doctor.name %> </h1>

<h1> <%= @doctor.department %> </h1>

<h2> Appointments: </h2>

<ul> 
    <% @doctor.appointments.each do |appointment| %> 
        <li> <%= link_to appointment.patient.name, patient_path(@patient) %> - <%= appointment.appointment_datetime.strftime("%B %d, %Y at %H:%M") %> </li>
    <% end %>
</ul>